.mobjs/masktools_filters_support_makediff_functions.o: masktools/filters/support/makediff/functions.cpp masktools/filters/support/makediff/filter.h masktools/common/base/filter.h common/functions/functions.h common/utils/utils.h common/constraints/constraints.h common/plane/plane.h common/filter/filter.h common/params/params.h common/clip/clip.h common/frame/frame.h common/parser/parser.h common/parser/symbol.h masktools/common/common.h masktools/common/params/params.h masktools/common/clip/clip.h
	$(CXX) $(CXXFLAGS) -c masktools/filters/support/makediff/functions.cpp -o $@
.mobjs/masktools_filters_support_makediff_functions.s.o: masktools/filters/support/makediff/functions.asm
	$(YASM) $(ASMFLAGS) masktools/filters/support/makediff/functions.asm -o $@

# Nobody knows the trouble I've seen, no... no... nooo...
.mobjs:
	-mkdir .mobjs

